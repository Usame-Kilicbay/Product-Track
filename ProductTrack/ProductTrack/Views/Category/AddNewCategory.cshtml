@model ProductTrack.Models.Entity.TBL_Categories
@{
    ViewBag.Title = "AddNewCategory";
    Layout = "~/Views/Shared/_MainLayout.cshtml";
}

<form method="post">
    <div class="form-group">
        <label for="category-name">Category Name</label>
        <input id="category-name" type="text" class="form-control" name="CategoryName" />
    </div>
    <div class="form-group">
        <button type="submit" class="btn btn-success">
            <span class="glyphicon glyhicon-plus"></span>
            &nbsp;Add
        </button>
    </div>
</form>

